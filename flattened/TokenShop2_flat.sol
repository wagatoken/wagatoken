library OracleLib {
    error OracleLib__StalePriceData();
    uint256 private constant TIMEOUT = 1 hours; 

    function stalePriceCheckLatestRoundData(
        AggregatorV3Interface priceFeed
    ) public view returns (uint80, int256, uint256, uint256, uint80) {
        (
            uint80 roundId,
            int256 answer,
            uint256 startedAt,
            uint256 updatedAt,
            uint80 answeredInRound
        ) = priceFeed.latestRoundData();

        if (block.timestamp - updatedAt > TIMEOUT) { // Yohaness to check conversion to seconds
            revert OracleLib__StalePriceData();
        }
        return (roundId, answer, startedAt, updatedAt, answeredInRound);
    }
}

